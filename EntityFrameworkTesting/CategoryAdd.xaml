<Window x:Class="EntityFrameworkTesting.CategoryAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EntityFrameworkTesting"
        mc:Ignorable="d"
        Title="CategoryAdd" Height="131" Width="270">
    <Grid>
        <TextBox x:Name="CategoryText" HorizontalAlignment="Center" Margin="0,17,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="215"/>
        <Button x:Name="Add" Content="Añadir" HorizontalAlignment="Left" Height="39" Margin="27,47,27,17" VerticalAlignment="Top" Width="101" Click="Add_Click"/>
        <Button x:Name="Cancel" Content="Cancelar" HorizontalAlignment="Left" Height="39" Margin="141,47,0,0" VerticalAlignment="Top" Width="101" Click="Cancel_Click"/>

    </Grid>
</Window>
